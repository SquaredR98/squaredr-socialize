{
  "name": "squaredr-socialize-backend",
  "version": "1.0.0",
  "description": "Node based backend for the social media application.",
  "main": "index.js",
  "scripts": {
    "start": "nodemon ./build",
    "dev": "nodemon -r tsconfig-paths/register src/index.ts | ./node_modules/.bin/bunyan",
    "build": "ttsc -p .",
    "lint:check": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "prettier:check": "prettier --check 'src/**/*.{ts,json}'",
    "prettier:fix": "prettier --write 'src/**/*.{ts,json}'"
  },
  "keywords": [
    "node",
    "nodejs",
    "express",
    "mongodb",
    "mongoose",
    "API",
    "RestAPI",
    "backend"
  ],
  "author": "Ravi Ranjan",
  "license": "ISC",
  "dependencies": {
    "@socket.io/redis-adapter": "^8.0.0",
    "bunyan": "^1.8.15",
    "compression": "^1.7.4",
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^6.0.1",
    "hpp": "^0.2.3",
    "http-status-codes": "^2.2.0",
    "mongoose": "^6.8.3",
    "redis": "^4.5.1",
    "socket.io": "^4.5.4",
    "ttypescript": "^1.5.15",
    "typescript": "^4.9.4",
    "typescript-transform-paths": "^3.4.6"
  },
  "devDependencies": {
    "@types/bunyan": "^1.8.8",
    "@types/compression": "^1.7.2",
    "@types/cookie-session": "^2.0.44",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/hpp": "^0.2.2",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2"
  }
}
